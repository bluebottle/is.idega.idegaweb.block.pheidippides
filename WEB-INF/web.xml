<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">

	<!-- GolfRegistryFilter listens for member registry updates from IWMember -->
	<filter>
		<filter-name>ValitorFilter</filter-name>
		<filter-class>is.idega.idegaweb.pheidippides.servlet.filter.ValitorFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>ValitorFilter</filter-name>
		<url-pattern>/registration/valitor/</url-pattern>
	</filter-mapping>

</web-app>